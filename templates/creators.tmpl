{{ define "content" }}
  <div class="lg:container mx-auto p-4">
    <h1 class="text-2xl font-semibold mb-4">Creators</h1>
    {{ range .Creators }}
      <div class="text-pretty border rounded-lg border-orange-700 bg-white p-4 mb-4">
        <h2 class="text-xl font-semibold">{{ .Name }}</h2>
      </div>
    {{ end }}
  </div>
{{ end }}

