{{ define "content" }}
  <div class="lg:container mx-auto p-4">
    <div class="text-pretty border rounded-lg border-orange-700 bg-white p-4 mb-4">
      <h2 class="text-xl font-semibold">{{ .Creator.Name }}</h2>
    </div>
    {{ range .Notes }}
      {{ template "note" . }}
    {{ end }}
    {{ range .Series }}
      {{ template "series_card" . }}
    {{ end }}
  </div>
{{ end }}

